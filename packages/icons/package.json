{
  "name": "@bereghici/icons",
  "version": "0.0.1",
  "description": "Generated icons from svg",
  "scripts": {
    "build": "bereghici-scripts build --bundle esm --environment BUILD_INPUT:src/*.tsx",
    "lint": "bereghici-scripts lint 'src/**/*.{ts,tsx}'",
    "validate": "bereghici-scripts validate",
    "generate": "node ./generate.js"
  },
  "type": "module",
  "exports": {
    ".": "./dist",
    "./*": "./dist/esm/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/esm/*"
      ]
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0"
  },
  "devDependencies": {
    "@bereghici/bereghici-scripts": "0.0.1",
    "@bereghici/eslint-config": "0.0.1",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "@types/react": "17.0.11",
    "camelcase": "^6.1.0",
    "chalk": "^4.1.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.7",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "typescript": "^4.3.2"
  }
}
