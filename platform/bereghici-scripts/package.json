{
  "name": "@bereghici/bereghici-scripts",
  "version": "0.0.1",
  "description": "CLI for common scripts.",
  "engines": {
    "node": ">=10.18",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "bin": {
